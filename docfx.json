{
  "build": {
    "content": [
      {
        "files": [
          "*.md",
          "Nodify/**/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**/*"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default"
    ],
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "overwrite": [
      {
        "files": [
          "apidoc/**/*"
        ],
        "exclude": [
          "*.md"
        ]
      }
    ],
    "markdownEngineName": "markdig",
    "cleanupCacheHistory": false,
    "keepFileLinksWithoutTitle": false,
    "disableGitFeatures": false,
    "postProcessors": [],
    "noLangKeyword": false,
    "keepRawFiles": false,
    "externalReference": [],
    "forceRebuild": false,
    "incremental": true,
    "disableDefaultFilter": false,
    "filter": null,
    "fileMetadata": null,
    "globalMetadata": null,
    "templateMetadata": null,
    "markdownEngineProperties": null,
    "disableGitFeatures": false,
    "disableSearchIndex": false
  },
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Nodify/Nodify.csproj"
          ],
          "exclude": [
            "obj/**",
            "bin/**"
          ]
        }
      ],
      "dest": "api",
      "disableDefaultFilter": false
    }
  ]
}