{
  "build": {
    "content": [
      {
        "files": [
          "Docs/**.md",
          "Docs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": []
      }
    ],
    "dest": "_site",
    "template": [
      "default"
    ],
    "metadata": [
      {
        "outputFormat": "markdown"
      }
    ],
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}